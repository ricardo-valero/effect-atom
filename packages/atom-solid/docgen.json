{
  "exclude": ["src/internal/**/*.ts"],
  "theme": "mikearnaldi/just-the-docs",
  "parseCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "target": "es2015",
    "lib": ["es2015"],
    "paths": {
      "@effect-atom/atom": ["./src/index.ts"],
      "@effect-atom/atom/*": ["./src/*"]
    }
  },
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "target": "es2015",
    "lib": ["es2015"],
    "paths": {
      "@effect-atom/atom": ["./src/index.ts"],
      "@effect-atom/atom/*": ["./src/*"]
    }
  }
}
